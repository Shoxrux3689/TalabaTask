@model CreateScienceModel


<form class="row g-3" style="width: 400px" asp-controller="Sciences" asp-action="CreateScience" method="post">
    <div>
        <input name="Name" class="form-control" asp-for="Name" type="text" placeholder="Name" />
        <span asp-validation-for="Name"></span>
    </div>
    <button class="form-control" type="submit">Create Science</button>
</form>


